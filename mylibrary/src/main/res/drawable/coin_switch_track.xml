<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:bottom="0dp" android:left="0dp" android:right="0dp" android:state_checked="true" android:top="0dp">
        <shape android:shape="rectangle">
            <size android:width="20dp" android:height="20dp" />
            <corners android:radius="20dp" />
            <!--<size android:width="30dp" android:height="30dp" />-->
            <!--<stroke android:width="4dp" android:color="#0000ffff" />-->
            <solid android:color="@color/blue_switch" />
        </shape>
    </item>
    <item android:bottom="0dp" android:left="0dp" android:right="0dp" android:state_checked="false" android:top="0dp">
        <shape android:shape="rectangle">
            <size android:width="20dp" android:height="20dp" />
            <corners android:radius="20dp" />
            <solid android:color="@color/colorGrey" />
        </shape>
    </item>
</selector>