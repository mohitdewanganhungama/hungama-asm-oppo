<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    >

    <View
        android:id="@+id/view30"
        android:layout_width="@dimen/dimen_0"
        android:layout_height="@dimen/dimen_15"
        android:layout_marginStart="@dimen/dimen_20"
        android:layout_marginEnd="@dimen/dimen_30"
        android:background="@drawable/bg_shimmer_item_view"
        android:layout_marginBottom="@dimen/dimen_20"
        app:layout_constraintBottom_toTopOf="@+id/view1"
        app:layout_constraintEnd_toStartOf="@+id/linearLayout2"
        app:layout_constraintStart_toStartOf="parent" />

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="@dimen/dimen_32"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/dimen_15"
        android:orientation="vertical"
        android:layout_marginBottom="@dimen/dimen_40"
        app:layout_constraintBottom_toTopOf="@+id/view5"
        app:layout_constraintEnd_toEndOf="parent">

        <com.google.android.material.imageview.ShapeableImageView
            android:layout_width="@dimen/dimen_30"
            android:layout_height="@dimen/dimen_30"
            android:background="@drawable/bg_gradient_placeholder"
            android:scaleType="centerCrop"
            app:shapeAppearanceOverlay="@style/ShapeAppearanceOverlay.App.Circle" />

        <com.google.android.material.imageview.ShapeableImageView
            android:layout_width="@dimen/dimen_30"
            android:layout_height="@dimen/dimen_30"
            android:layout_marginTop="@dimen/dimen_15"
            android:background="@drawable/bg_gradient_placeholder"
            android:scaleType="centerCrop"
            app:shapeAppearanceOverlay="@style/ShapeAppearanceOverlay.App.Circle" />

        <com.google.android.material.imageview.ShapeableImageView
            android:layout_width="@dimen/dimen_30"
            android:layout_height="@dimen/dimen_30"
            android:layout_marginTop="@dimen/dimen_15"
            android:background="@drawable/bg_gradient_placeholder"
            android:scaleType="centerCrop"
            app:shapeAppearanceOverlay="@style/ShapeAppearanceOverlay.App.Circle" />

        <com.google.android.material.imageview.ShapeableImageView
            android:layout_width="@dimen/dimen_30"
            android:layout_height="@dimen/dimen_30"
            android:layout_marginTop="@dimen/dimen_15"
            android:background="@drawable/bg_gradient_placeholder"
            android:scaleType="centerCrop"
            app:shapeAppearanceOverlay="@style/ShapeAppearanceOverlay.App.Circle" />

        <com.google.android.material.imageview.ShapeableImageView
            android:layout_width="@dimen/dimen_30"
            android:layout_height="@dimen/dimen_30"
            android:layout_marginTop="@dimen/dimen_15"
            android:background="@drawable/bg_gradient_placeholder"
            android:scaleType="centerCrop"
            app:shapeAppearanceOverlay="@style/ShapeAppearanceOverlay.App.Circle" />
    </LinearLayout>

    <View
        android:id="@+id/view1"
        android:layout_width="@dimen/dimen_130"
        android:layout_height="@dimen/dimen_25"
        android:layout_gravity="center"
        android:layout_marginStart="@dimen/dimen_20"
        android:background="@drawable/bg_shimmer_header"
        android:ellipsize="end"
        android:layout_marginBottom="@dimen/dimen_10"
        android:fontFamily="@font/sf_pro_text"
        android:lineSpacingExtra="@dimen/font_2"
        android:maxLines="1"
        android:textAlignment="center"
        android:textColor="@color/story_not_seen_text"
        android:textSize="@dimen/font_11"
        android:textStyle="normal"
        app:layout_constraintBottom_toTopOf="@+id/view2"
        app:layout_constraintStart_toStartOf="parent"
        app:lineHeight="@dimen/font_13" />

    <View
        android:id="@+id/view31"
        android:layout_width="@dimen/dimen_13"
        android:layout_height="@dimen/dimen_17"
        android:layout_marginStart="@dimen/dimen_20"
        android:background="@drawable/bg_simmer_icon"
        android:layout_marginBottom="@dimen/dimen_10"
        app:layout_constraintBottom_toTopOf="@+id/view3"
        app:layout_constraintStart_toStartOf="parent" />

    <View
        android:id="@+id/view2"
        android:layout_width="@dimen/dimen_170"
        android:layout_height="@dimen/dimen_18"
        android:layout_gravity="center"
        android:layout_marginStart="@dimen/dimen_5"
        android:background="@drawable/bg_shimmer_header"
        android:ellipsize="end"
        android:fontFamily="@font/sf_pro_text"
        android:lineSpacingExtra="@dimen/font_2"
        android:maxLines="1"
        android:layout_marginBottom="@dimen/dimen_10"
        android:textAlignment="center"
        android:textColor="@color/story_not_seen_text"
        android:textSize="@dimen/font_11"
        android:textStyle="normal"
        app:layout_constraintBottom_toTopOf="@+id/view3"
        app:layout_constraintStart_toEndOf="@+id/view31"
        app:lineHeight="@dimen/font_13" />

    <View
        android:id="@+id/view3"
        android:layout_width="@dimen/dimen_170"
        android:layout_height="@dimen/dimen_15"
        android:layout_gravity="center"
        android:layout_marginBottom="@dimen/dimen_30"
        android:layout_marginStart="@dimen/dimen_20"
        android:background="@drawable/bg_shimmer_header"
        android:ellipsize="end"
        android:fontFamily="@font/sf_pro_text"
        android:lineSpacingExtra="@dimen/font_2"
        android:maxLines="1"
        android:textAlignment="center"
        android:textColor="@color/story_not_seen_text"
        android:textSize="@dimen/font_11"
        android:textStyle="normal"
        app:layout_constraintBottom_toTopOf="@+id/view5"
        app:layout_constraintStart_toStartOf="parent"
        app:lineHeight="@dimen/font_13" />

    <View
        android:id="@+id/view5"
        android:layout_width="@dimen/dimen_0"
        android:layout_height="@dimen/dimen_2"
        android:layout_marginStart="@dimen/dimen_18"
        android:layout_marginEnd="@dimen/dimen_4"
        android:alpha="0.5"
        android:background="#FFFFFF"
        android:layout_marginBottom="@dimen/dimen_72"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/view8"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/view7" />

    <View
        android:id="@+id/view6"
        android:layout_width="@dimen/dimen_11"
        android:layout_height="@dimen/dimen_11"
        android:background="@drawable/bg_skeleton_seekbar"
        app:layout_constraintTop_toTopOf="@id/view5"
        app:layout_constraintBottom_toBottomOf="@id/view5"
        app:layout_constraintStart_toStartOf="@id/view5"
        android:layout_marginStart="@dimen/dimen_50"/>

    <View
        android:id="@+id/view7"
        android:layout_width="@dimen/dimen_21"
        android:layout_height="@dimen/dimen_21"
        android:layout_marginStart="@dimen/dimen_20"
        android:background="@drawable/bg_shimmer_view_icon"
        app:layout_constraintBottom_toBottomOf="@+id/view5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view5" />

    <View
        android:id="@+id/view8"
        android:layout_width="@dimen/dimen_20"
        android:layout_height="@dimen/dimen_11"
        android:layout_marginEnd="@dimen/dimen_25"
        android:background="@drawable/bg_shimmer_item_view"
        app:layout_constraintBottom_toBottomOf="@+id/view5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view5" />

</androidx.constraintlayout.widget.ConstraintLayout>